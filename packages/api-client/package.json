{
  "name": "@absolute-web/magento-api",
  "version": "0.0.12",
  "sideEffects": false,
  "homepage": "https://github.com/absolutewebservices/magento2",
  "bugs": {
    "url": "https://github.com/absolutewebservices/magento2/issues"
  },
  "license": "MIT",
  "server": "server/index.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib server && rollup -c",
    "dev": "rimraf lib server && rollup -c -w",
    "lint": "eslint ./src --ext .ts,.vue",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "partial:types": "node ./partial-types.js",
    "precommit": "lint-staged",
    "prepublish": "yarn build",
    "test": "jest",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "graphql": "^15.6.0",
    "graphql-tag": "^2.12.5",
    "isomorphic-fetch": "^3.0.0"
  },
  "peerDependencies": {
    "@absolute-web/vsf-core": "~0.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-graphql": "^1.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "dotenv": "^10.0.0",
    "graphql-tools": "^8.2.0",
    "jest-transform-graphql": "^2.1.0",
    "node-fetch": "^2.6.5",
    "typescript": "^4.4.3",
    "webpack": "^4.46.0"
  },
  "files": [
    "lib/**/*",
    "server/**/*",
    "types/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry":"https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/absolutewebservices/magento2.git"
  },
  "engines": {
    "node": ">=14.x"
  }
}
